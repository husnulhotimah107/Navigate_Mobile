<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentMain">

    <fragment
        android:id="@+id/fragmentMain"
        android:name="com.example.navigation_husnul.fragmentMain"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_fragmentMain_to_fragmentOther"
            app:destination="@id/fragmentOther" />
        <argument
            android:name="nama"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/fragmentOther"
        android:name="com.example.navigation_husnul.fragmentOther"
        android:label="fragment_other"
        tools:layout="@layout/fragment_other" />
</navigation>